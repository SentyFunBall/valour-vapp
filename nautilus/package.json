{
  "name": "nautilus",
  "productName": "Nautilus",
  "version": "0.7.3",
  "description": "Bots for Valour.",
  "main": "./src/home/start.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "npx @electron-forge/cli make --platform darwin",
    "make-linux": "npx @electron-forge/cli make --platform linux --targets deb",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "SentyFunball",
    "email": "sentyfb@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "icon": "./src/assets/icons/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Nautilus"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": ".src/assets/icons/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@ftnk/electron-prebuilt-compile": "^1.7.3",
    "electron": "11.2.0",
    "electron-compile": "^6.4.4",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "electron-prompt": "^1.6.2",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@ftnk/electron-prebuilt-compile": "^1.7.3",
    "electron": "11.2.0",
    "electron-compile": "^6.4.4",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "electron-squirrel-startup": "^1.0.0"
  }
}
